{
  "name": "togglefy",
  "version": "0.0.1-alpha.1",
  "description": "Togglefy is designed to add interactivity to web pages by toggling the classes of specified elements.",
  "author": "ux-ui.pro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ux-ui-pro/togglefy.git"
  },
  "bugs": {
    "url": "https://github.com/ux-ui-pro/togglefy/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "targets": {
    "main": {
      "source": "src/index.js"
    },
    "module": {
      "source": "src/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist .parcel-cache",
    "serve": "parcel src/index.html --dist-dir dist --cache-dir .parcel --no-source-maps --open",
    "build": "yarn clean && parcel build",
    "lint:js": "eslint --ext .js",
    "lintfix": "yarn lint:js --fix"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "parcel": "^2.12.0"
  },
  "keywords": []
}
